<Window x:Class="VisualQueryApplication.QueryResultsViewer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VisualQueryApplication"
        xmlns:vm="clr-namespace:VisualQueryApplication.ViewModels"
        mc:Ignorable="d"
        Title="QueryResultsViewer" Height="300" Width="750">

    <Grid>
        <DataGrid ItemsSource="{Binding QueryResult}" />
    </Grid>
</Window>
